 <style>
        .offcanvas-menu-wrapper {
        margin-top: 60px; /* Adjust based on your navbar height */
        z-index: 1001;
        position: relative;
        }
        .header {
        z-index: 1000;
        position: relative;
        }
        .offcanvas-menu-wrapper {
        display: none;
        background: white;
        position: fixed;
        top: 0;
        right: 0;
        width: 100%;
        height: 100vh;
        overflow-y: auto;
        z-index: 999;
    }
    
    .offcanvas-menu-wrapper.active {
        display: block;
    }
    .offcanvas__nav__option {
        display: flex;
        justify-content: space-around;
        align-items: center;
        padding: 15px 0;
        border-bottom: 1px solid #eee;
    }
    
    .offcanvas__nav__option a {
        color: #000;
        font-weight: 500;
        font-size: 14px;
    }
    .offcanvas-close {
      text-align: right;
      padding: 15px 20px;
      font-size: 24px;
      cursor: pointer;
      color: #000;
    }

    .offcanvas-close i {
