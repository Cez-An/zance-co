                            data-product-id="<%= item.productId._id %>"
                            data-base-price="<%= item.productId.offerPrice %>">
                            <div class="row g-0 align-items-center px-4 py-2">
                                <div class="col-md-2 text-center">
                                    <img src="<%= item.productImage %>" alt="<%= item.productId.name %>"
                                        class="img-fluid rounded" style="max-height: 80px; object-fit: contain;">
                                </div>
                                <div class="col-12 col-md-10">
                                     <div class="flex-grow-1" style="min-width: 0;">
                                            <h6 class="fw-bold mb-2 text-dark text-truncate"
                                                style="font-size: 1.05rem;">
                                                <%= item.productId.name %>
                                            </h6>
                                            <input type="hidden" id="userId" value="<%= user._id %>">
                                        </div>
                                    <div class="d-flex align-items-center justify-content-center justify-content-md-end flex-nowrap gap-3">
                                       
                                        <div class="d-flex align-items-center gap-3 flex-shrink-0">
                                            <div class="quantity-control d-flex align-items-center gap-2">
                                                <button class="quantity-btn"
                                                    onclick="updateQuantity('<%= item.productId._id %>', -1)">
                                                    <i class="fa-solid fa-minus" style="font-size: 12px;"></i>
                                                </button>
                                                <span class="quantity-display">
                                                    <%= item.quantity %>
                                                </span>
                                                <button class="quantity-btn"
                                                    onclick="updateQuantity('<%= item.productId._id %>', 1)">
                                                    <i class="fa-solid fa-plus" style="font-size: 12px;"></i>
                                                </button>
                                            </div>
                                            <p class="mb-0 fw-bold text-dark total-price"
                                                style="font-size: 15px; width: 70px; flex-shrink: 0;">
                                                ₹ <%= (item.quantity * item.productId.offerPrice).toLocaleString('en-IN')
                                                    %>
